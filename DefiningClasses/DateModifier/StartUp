//Create a class DateModifier, which stores the difference of the days between two dates.
//It should have a method which takes two string parameters representing a date as strings 
//and calculates the difference in the days between them.

using System;

namespace _5.DateModifier
{
	class StartUp
	{
		static void Main(string[] args)
		{
			string firstDate = Console.ReadLine();
			string secondDate = Console.ReadLine();

			DateModifier dm = new DateModifier();
			dm.Modifier(firstDate, secondDate);			
		}
	}
}
