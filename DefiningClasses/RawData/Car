namespace _7.RawData
{
	public class Car
	{
		public Car(string model, Engine engine, Cargo cargo, Tire[] tires)
		{
			this.Model = model;
			this.Tires = tires;
			this.Engine = engine;
			this.Cargo = cargo;
		}
		public string Model { get; set; }

		public Tire[] Tires { get; set; }

		public Engine Engine { get; set; }

		public Cargo Cargo { get; set; }
	}	
}
